INSERT
{
  ?x ?targetProp ?y.

  GRAPH ${TARGET_GRAPH} {
    ?x ?targetProp ?y
  }
}
WHERE
{
  { ?srcProp (owl:equivalentProperty|^owl:equivalentProperty) ?targetProp }
  FILTER ( ?srcProp != ?targetProp )
  FILTER ( TRUE || STRSTARTS ( STR(?targetProp), STR (${TARGET_NS}) ) )

  { ?x ?srcProp ?y }
}
