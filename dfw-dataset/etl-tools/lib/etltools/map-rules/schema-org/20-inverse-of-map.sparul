# Rule Name: schema:inverseOf
#
INSERT
{
  ?y ?targetProp ?x.

  GRAPH ${TARGET_GRAPH} {
    ?y ?targetProp ?x
  }
}
WHERE
{
  { ?srcProp schema:inverseOf ?targetProp }
  FILTER ( ?srcProp != ?targetProp )
  # FILTER ( STRSTARTS ( STR(?targetProp), STR (${TARGET_NAMESPACE}) ) )

  { ?x ?srcProp ?y }
}
